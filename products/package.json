{
  "name": "products",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "deploy": "sls deploy",
    "build:deploy": "tsc && yarn deploy",
    "build:watch": "tsc --watch",
    "deps:upgrade-local": "yarn upgrade @alexgusevserg/shared",
    "docs:dev": "vite --config docs/vite.config.js",
    "docs:build": "tsc && vite build --config docs/vite.config.js",
    "docs:preview": "vite preview  --config docs/vite.config.js",
    "test:watch": "vitest watch --config test/vite.config.ts",
    "test:coverage": "vitest --config test/vite.config.ts run --coverage"
  },
  "engines": {
    "node": "^18"
  },
  "dependencies": {
    "@alexgusevserg/shared": "^1.0.5",
    "@middy/core": "^3.4.0",
    "@middy/http-error-handler": "^4.2.6",
    "@middy/http-json-body-parser": "^3.4.0",
    "@middy/validator": "^4.2.6",
    "ajv": "^8.12.0",
    "deepmerge": "^4.3.0",
    "http-errors": "^2.0.0",
    "serverless-s3-sync": "^3.1.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-content": "^1.0.3",
    "@serverless/typescript": "^3.27.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/http-errors": "^2.0.1",
    "@types/node": "^18",
    "@vitest/coverage-c8": "^0.29.2",
    "esbuild": "^0.17.10",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "swagger-ui": "4.5.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vitest": "^0.29.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
